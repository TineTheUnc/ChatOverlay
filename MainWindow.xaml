<Window x:Class="ChatOverlay.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChatOverlay"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="300">
	<StackPanel Margin="20">
		<Button x:Name="ImportButton" Content="Import client_secret" Click="Import_File" Margin="0 0 0 10" Background="ForestGreen"/>
		<Button x:Name="AuthorizationButton" Content="Authorization" Click="Authorization" Margin="0 0 0 10" Background="ForestGreen"/>
		<TextBlock Text="Live id: " Margin="0 0 0 10" FontWeight="Bold">
			<TextBox x:Name="InputBox"
			 Width="200"
			 Height="15"/>
		</TextBlock>
		<Button x:Name="StartButton" Content="Start" Click="Start" Margin="0 0 0 10" Background="ForestGreen"/>
	</StackPanel>
</Window>
